{
  "name": "fake-company-mfe",
  "private": true,
  "scripts": {
    "start": "concurrently -p \"none\" \"nx run-many --target=start --all --parallel --maxParallel=10\" \"pnpm run json-server\"",
    "build": "nx run-many --target=build --all --parallel",
    "test": "nx run-many --target=test --all --parallel",
    "e2e": "start-server-and-test start http-get://localhost:1337 cy-open",
    "e2e-ci": "start-server-and-test start http-get://localhost:1337 cy-run",
    "cy-open": "nx run-many --target=cy-open --projects=connect-e2e --parallel",
    "cy-run": "nx run-many --target=cy-run --projects=connect-e2e --parallel",
    "json-server": "json-server ./tools/json-server/index.js --watch --routes ./tools/json-server/routes.json --port 4000 --delay 1000"
  },
  "engines": {
    "node": ">=10",
    "pnpm": ">=5"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@babel/runtime": "^7.12.5",
    "@nrwl/cli": "^11.2.8",
    "@nrwl/nx-cloud": "^11.1.2",
    "@nrwl/tao": "^11.2.8",
    "@nrwl/workspace": "^11.2.8",
    "@storybook/react": "^6.1.16",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/react-hooks": "5.0.3",
    "@types/jest": "26.0.20",
    "@types/node": "13.13.40",
    "@types/redux-mock-store": "^1.0.2",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack-dev-server": "3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "axios-mock-adapter": "^1.18.1",
    "babel-eslint": "^10.0.0",
    "babel-jest": "26.2.2",
    "babel-loader": "8.2.2",
    "bundle-loader": "0.5.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.8.3",
    "cypress": "^5.5.0",
    "eslint": "^7.5.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^3.9.0",
    "fork-ts-checker-webpack-plugin": "6.1.0",
    "html-webpack-plugin": "5.0.0-beta.6",
    "husky": "4.3.8",
    "import-sort-style-module": "^6.0.0",
    "jest": "26.6.3",
    "json-server": "^0.16.3",
    "lint-staged": "10.5.3",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "^0.0.6",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^1.12.0",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "4.1.3",
    "webpack": "5.20.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
